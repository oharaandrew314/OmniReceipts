<h1>New Receipt</h1>

<%= form_for @receipt do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>

  <p>
    <%= f.label :store %><br>
    <%= f.text_field :store %>
  </p>

  <p>
    <%= f.label :date %><br>
    <input type="date" name="date"/>
  </p>

  <p>
    <%= f.submit %>
    <%= link_to "Cancel", receipts_path =%>
  </p>
<% end %>
